    "np": "^2.9.0",
    "prettier-standard": "^4.1.0",
    "sqlite3": "^3.1.8"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/integration/bootstrap.js test/integration/*.test.js",
